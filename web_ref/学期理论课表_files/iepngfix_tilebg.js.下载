var IEPNGFix=window.IEPNGFix||{};IEPNGFix.tileBG=function(n,t,i){var g=this.data[n.uniqueID],v=Math.max(n.clientWidth,n.scrollWidth),a=Math.max(n.clientHeight,n.scrollHeight),w=n.currentStyle.backgroundPositionX,p=n.currentStyle.backgroundPositionY,b=n.currentStyle.backgroundRepeat,it,f,e,k,rt,ut,tt,d;g.tiles||(g.tiles={elm:n,src:"",cache:[],img:new Image,old:{}});var r=g.tiles,h=r.img.width,s=r.img.height;if(t){if(!i&&t!=r.src)return r.img.onload=function(){this.onload=null,IEPNGFix.tileBG(n,t,1)},r.img.src=t}else r.src&&(i=1),h=s=0;if(r.src=t,i||v!=r.old.w||a!=r.old.h||w!=r.old.x||p!=r.old.y||b!=r.old.r){it={top:"0%",left:"0%",center:"50%",bottom:"100%",right:"100%"},f=it[w]||w,e=it[p]||p,(k=f.match(/(\d+)%/))&&(f=Math.round((v-h)*(parseInt(k[1])/100))),(k=e.match(/(\d+)%/))&&(e=Math.round((a-s)*(parseInt(k[1])/100))),f=parseInt(f),e=parseInt(e),rt={repeat:1,"repeat-x":1}[b],ut={repeat:1,"repeat-y":1}[b],rt&&(f%=h,f>0&&(f-=h)),ut&&(e%=s,e>0&&(e-=s)),this.hook.enabled=0,{relative:1,absolute:1}[n.currentStyle.position]||(n.style.position="relative");var l=0,o,et=rt?v:f+.1,c,ft=ut?a:e+.1,y,u,nt;if(h&&s)for(o=f;o<et;o+=h)for(c=e;c<ft;c+=s)nt=0,r.cache[l]||(r.cache[l]=document.createElement("div"),nt=1),tt=Math.max(0,o+h>v?v-o:h),d=Math.max(0,c+s>a?a-c:s),y=r.cache[l],u=y.style,u.behavior="none",u.left=o-parseInt(n.currentStyle.paddingLeft)+"px",u.top=c+"px",u.width=tt+"px",u.height=d+"px",u.clip="rect("+(c<0?0-c:0)+"px,"+tt+"px,"+d+"px,"+(o<0?0-o:0)+"px)",u.display="block",nt&&(u.position="absolute",u.zIndex=-999,n.firstChild?n.insertBefore(y,n.firstChild):n.appendChild(y)),this.fix(y,t,0),l++;while(l<r.cache.length)this.fix(r.cache[l],"",0),r.cache[l++].style.display="none";this.hook.enabled=1,r.old={w:v,h:a,x:w,y:p,r:b}}},IEPNGFix.update=function(){var t,n;for(t in IEPNGFix.data)n=IEPNGFix.data[t].tiles,n&&n.elm&&n.src&&IEPNGFix.tileBG(n.elm,n.src)},IEPNGFix.update.timer=0,window.attachEvent&&!window.opera&&window.attachEvent("onresize",function(){clearTimeout(IEPNGFix.update.timer),IEPNGFix.update.timer=setTimeout(IEPNGFix.update,100)})